<h1>Create a Preference</h1>
<%= form_tag admin_preferences_path, method: "post" do %>
<%= label_tag :song_sort_order %>
<%= text_field_tag "song_sort_order", @preference.song_sort_order %>
<br>
<%= label_tag :artist_sort_order %>
<%= text_field_tag "artist_sort_order", @preference.artist_sort_order %>
<br>
<%= label_tag :allow_create_artists %>
<%= text_field_tag "allow_create_artists", @preference.allow_create_artists %>
<br>
<%= label_tag :allow_create_songs %>
<%= text_field_tag "allow_create_songs", @preference.allow_create_songs %>
<br>
<%= submit_tag "Create Preference" %>
<% end %>
