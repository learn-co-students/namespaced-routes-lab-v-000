<h1>Adjust Access</h1>
<%= form_for @preference, url: admin_access_path(@preference) do |f| %>
  <%= f.label :allow_create_artists %>
  <br>
  True <%= f.radio_button(:allow_create_artists, true) %><br>
  False <%= f.radio_button(:allow_create_artists, false) %><br>
  <br>
  <%= f.label :allow_create_songs %>
  <br>
  True <%= f.radio_button(:allow_create_songs, true) %><br>
  False <%= f.radio_button(:allow_create_songs, false) %><br>
  <br>
  <%= f.submit %>
<% end %>
