<h1>Admin - Access</h1>
<%= form_for @preference, url: admin_access_path(@preference) do |f| %>
  
  <%= f.label :allow_create_artists %>
  <p>Allow: <%= f.radio_button :allow_create_artists, true %> </p>
  <p>Not Allow: <%= f.radio_button :allow_create_artists, false %> </p>

  <%= f.label :allow_create_songs %>
  <p>Allow: <%= f.radio_button :allow_create_songs, true %> </p>
  <p>Not Allow: <%= f.radio_button :allow_create_songs, false %> </p>

  <%= f.submit %>
<% end %>